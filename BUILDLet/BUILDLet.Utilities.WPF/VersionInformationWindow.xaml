<Window x:Class="BUILDLet.Utilities.WPF.VersionInformationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:app="clr-namespace:BUILDLet.Utilities.WPF"
        xmlns:prop="clr-namespace:BUILDLet.Utilities.WPF.Properties"
        Title="{x:Static prop:Resources.VersionInformationWindowTitle}" Height="140" Width="400" ResizeMode="NoResize" KeyDown="Window_KeyDown">
    <StackPanel>
        <Label Name="NameLabel" Content="Name"/>
        <Label Name="VersionLabel" Content="Version"/>
        <Button Name="OKButton" Content="OK" Margin="140,15" Height="25" IsDefault="True" Click="OKButton_Click"/>

    </StackPanel>
</Window>
